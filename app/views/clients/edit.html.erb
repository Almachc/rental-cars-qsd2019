<h1>Editar cliente</h1>

<%= form_with model: @client, local: true do |f| %>
    <%= f.label :name, 'Nome' %>
    <%= f.text_field :name %>
    <%= f.label :cpf, 'CPF' %>
    <%= f.text_field :cpf %>
    <%= f.label :email, 'Email' %>
    <%= f.text_field :email %>
    <%= f.submit 'Enviar' %>
<% end %>